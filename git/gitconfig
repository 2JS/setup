[commit]
	gpgsign = true
[core]
	ignorecase = false
	pager = delta
[delta]
	dark = true
	line-numbers = true
	navigate = true
	side-by-side = true
[diff]
	colorMoved = default
[fetch]
	prune = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	required = true
	smudge = git-lfs smudge -- %f
[init]
	defaultBranch = main
[interactive]
	diffFilter = delta --color-only --features=interactive
[merge]
	conflictstyle = diff3
[pull]
	rebase = true
[push]
	autosetupremote = true
[rebase]
	autostash = true
[tag]
	forceSignAnnotated = true
